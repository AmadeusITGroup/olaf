{
  "mcpServers": {
    "aws-docs": {
      "command": "uvx",
      "args": ["awslabs.aws-documentation-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": ["search_documentation", "get_service_info"]
    },
    "filesystem": {
      "command": "uvx",
      "args": ["mcp-server-filesystem@latest"],
      "env": {},
      "disabled": false,
      "autoApprove": ["read_file", "list_directory"]
    },
    "web-search": {
      "command": "uvx", 
      "args": ["mcp-server-web-search@latest"],
      "env": {
        "SEARCH_API_KEY": "your-api-key-here"
      },
      "disabled": true,
      "autoApprove": []
    }
  }
}